<%= form_with url: player_pool_entries_path,
              remote: true do |f| %>
  <%= hidden_field_tag 'player_pool_entry[player_pool_id]', entry.player_pool_id %>
  <%= hidden_field_tag 'player_pool_entry[player_id]', entry.player_id %>
  <%= f.submit "+", class: "h-8 w-8 pxauto rounded bg-teal-800 text-white text-center align-middle font-black" %>
<% end %>
