<% if entry.persisted? %>
  <% if entry.is_excluded %>
    <%= render partial: 'players/excluded_from_pool_actions', locals: { entry: entry } %>
  <% else %>
    <%= render partial: 'players/added_to_pool_actions', locals: { entry: entry } %>
  <% end %>
<% else %>
  <%= render partial: 'players/out_of_pool_actions', locals: { entry: entry } %>
<% end %>
